<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <!-- Refer to https://docs.liquibase.com/change-types/community/create-table.html -->

    <changeSet id="33d4e9bc-7210-47c3-9617-c56ab7c0750d" author="juan.zorrilla" failOnError="true">

        <createTable tableName="tiger">
            <column name="id" type="UUID">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="mother" type="varchar(255)"/>
            <column name="father" type="varchar(255)"/>
            <column name="name" type="varchar(255)"/>
            <column name="gender" type="varchar(255)"/>
            <column name="weight" type="numeric()"/>
            <column name="age" type="varchar(255)"/>
            <column name="height" type="varchar(255)"/>
            <column name="arrive_date" type="datetime"/>
        </createTable>
    </changeSet>
</databaseChangeLog>